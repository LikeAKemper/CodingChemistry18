{
  "extends": ["tslint:recommended"],
  "rules": {
    "object-literal-sort-keys": false,
    "trailing-comma": true,
    "interface-name": {
      "severity": "warning"
    },
    "ordered-imports": {
      "severity": "warning"
    },
    "no-console": {
      "severity": "warning"
    },
    "no-unused-variable": {
      "severity": "warning"
    },
    "no-unused-expression": {
      "severity": "warning"
    },
    "comment-format": {
      "severity": "warning"
    },
    "member-access": [true, "check-accessor"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "prefer-switch": true,
    "member-ordering": {
      "severity": "warning"
    },
    "object-literal-key-quotes": [true, "as-needed"]
  }
}